{
  "expo": {
    "name": "GoogleAuthDemo",
    "slug": "google-auth-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.infiniteoptions.googleauthdemo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["$(PRODUCT_BUNDLE_IDENTIFIER)"],
            "CFBundleURLName": "google"
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.infiniteoptions.googleauthdemo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "googleauthdemo",
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "$(GOOGLE_URL_SCHEME)"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d4a2a545-5903-4bea-acab-e3d08eed24e2"
      }
    }
  }
}
